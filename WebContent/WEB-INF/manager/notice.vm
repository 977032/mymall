<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<link href="styles/global-reset.css" rel="stylesheet" type="text/css" />
<link type="text/css" href="styles/style.css" rel="stylesheet" />
<script type="text/javascript" src="scripts/jquery-1.4.2.min.js"></script>
<script type="text/javascript" src="scripts/simpla.jquery.configuration.js"></script>
<title>提示</title>
</head>
<body>
<div id="body-wrapper">
  #parse("/WEB-INF/manager/menu.vm")
  <div id="main-content">
    <p id="page-intro">提示，关于操作问题：</p>
    <div class="clear"></div>
    <div class="content-box">
      <div class="content-box-header">
        <h3>提示：</h3>
        <div class="clear"></div>
      </div>
      <div class="content-box-content">
        <div class="tab-content default-tab" id="tab1">
          <div class="notification #if($notice.status) success#else error#end png_bg"><a class="close" href="#"><img title="Close this notification" alt="close" src="images/cross_grey_small.png"></a>
            <div>$!{notice.status}<br />
              $!{notice.code}<br />
              $!{notice.title}<br />
              $!{notice.body}<br />
            </div>
          </div>
        </div>
        <div class="tab-content" id="tab2"></div>
      </div>
    </div>
    <div class="clear"></div>
    #include("/WEB-INF/manager/footer.vm")
  </div>
</div>
</body>
</html>
